<scrollable-settings-container>
	
	<dropdown-list-setting 
		id="colorPresetsDropDown"
		text="Colors"
		value="listChoice"
		options="listOptions"
		apply-on-change="true"
		set-event="setEvent"
	/>
	
	<color-setting 
		text="Left Saber" 
		id="leftSaberColorSelector" 
		value="leftSaberColorVal"
	/>
	
	
	<button 
		text="New Preset" 
		on-click="newPresetButtonClicked"
	/>
	
</scrollable-settings-container>